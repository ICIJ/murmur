{
  "name": "@icij/murmur",
  "version": "3.2.0",
  "private": false,
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue",
    "./docs"
  ],
  "description": "Murmur is ICIJ's Design System for Bootstrap 4 and Vue.js",
  "author": "promera@icij.org",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build:docs && yarn build:lib",
    "lint": "vue-cli-service lint",
    "deploy": "node deploy.mjs",
    "build:docs": "vue-cli-service build --dest dist/docs",
    "build:lib": "vue-cli-service build --target lib --dest dist/lib 'lib/main.js'",
    "postpublish": "yarn deploy",
    "prepublishOnly": "yarn build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "bootstrap": "^4.6.2",
    "clipboard": "^2.0.11",
    "d3": "7.6.1",
    "d3-geo-projection": "^4.0.0",
    "jsonp": "^0.2.1",
    "lodash": "4.17.21",
    "mutationobserver-shim": "^0.3.7",
    "path-browserify": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "topojson": "^3.0.2",
    "vue-headroom": "^0.10.1"
  },
  "peerDependencies": {
    "bootstrap-vue": "2.22.0",
    "vue": "^2.6.12",
    "vue-i18n": "^8.25.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^1.2.2",
    "@vue/vue2-jest": "^28",
    "@vuepress/markdown": "^1.9.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "bootstrap-vue": "2.22.0",
    "chalk": "^4.1.2",
    "chroma-js": "^2.1.1",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "front-matter": "^4.0.2",
    "get-sass-vars": "^3.4.0",
    "gh-pages": "^4.0.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.3",
    "jest-canvas-mock": "^2.3.1",
    "json-loader": "^0.5.7",
    "loader-utils": "^2.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-highlightjs": "^3.3.1",
    "ora": "^6.1.2",
    "sass": "^1.54.0",
    "sass-loader": "^10.1.0",
    "toml": "^3.0.0",
    "url-join": "^4.0.1",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-cli-plugin-i18n": "^2.1.4",
    "vue-docgen-api": "^4.41.2",
    "vue-i18n": "^8.25.1",
    "vue-jest": "^3.0.7",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock",
      "<rootDir>/tests/unit/setup.mocks.js"
    ],
    "globalSetup": "<rootDir>/tests/unit/setup.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(d3)|(bootstrap-vue))/",
      "<rootDir>/node_modules/(?!d3-.*)/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/lib/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/unit/**/*.spec.js"
    ],
    "coverageDirectory": "<rootDir>/tests/unit/coverage",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.(js|vue)"
    ],
    "testURL": "http://localhost/"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "directories": {
    "lib": "lib",
    "doc": "docs"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "files": [
    "lib/**/*",
    "dist/lib/**/*"
  ],
  "homepage": "https://icij.github.io/murmur/",
  "license": "MIT",
  "main": "dist/lib/murmur.umd.min.js",
  "style": "dist/lib/murmur.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/icij/murmur"
  }
}
