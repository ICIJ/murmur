{
  "name": "icij-vue-collection",
  "version": "0.1.13",
  "description": "A beautiful Design System for Bootstrap 4 and Vue.js",
  "author": "promera@icij.org",
  "main": "dist/lib/icij-vue-collection.umd.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/icij/vue-collection"
  },
  "files": [
    "lib/**/*",
    "dist/lib/**/*"
  ],
  "directories": {
    "lib": "lib",
    "doc": "docs"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "postpublish": "yarn deploy",
    "serve": "vue-cli-service serve",
    "build": "yarn build:docs && yarn build:lib",
    "build:docs": "vue-cli-service build --dest dist/docs",
    "build:lib": "vue-cli-service build --target lib --dest dist/lib 'lib/main.js'",
    "deploy": "node deploy.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.1",
    "@fortawesome/free-brands-svg-icons": "^5.1",
    "@fortawesome/free-regular-svg-icons": "^5.1.1",
    "@fortawesome/free-solid-svg-icons": "^5.1",
    "@fortawesome/vue-fontawesome": "^0.0.22",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "clipboard": "^2.0.4",
    "es6-symbol": "^3.1.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "promise-polyfill": "^8.1.0",
    "vue": "^2.5.17",
    "vue-headroom": "^0.9.0",
    "vue-router": "^3.0.1",
    "vue-social-sharing": "^2.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "@vuepress/markdown": "^1.0.0-alpha.0",
    "@vuepress/markdown-loader": "^1.0.0-alpha.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.1",
    "chroma-js": "^1.4.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "front-matter": "^3.0.1",
    "gh-pages": "^2.0.1",
    "gray-matter": "^4.0.1",
    "json-loader": "^0.5.7",
    "loader-utils": "^1.1.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-highlightjs": "^3.0.0",
    "node-sass": "^4.10.0",
    "ora": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-extract": "^2.1.0",
    "sass-extract-loader": "^1.1.0",
    "sass-loader": "^7.0.1",
    "toml": "^2.3.3",
    "url-join": "^4.0.0",
    "vue-docgen-api": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
