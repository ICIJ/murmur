{
  "name": "@icij/murmur",
  "version": "3.3.0",
  "private": false,
  "description": "Murmur is ICIJ's Design System for Bootstrap 4 and Vue.js",
  "author": "promera@icij.org",
  "type": "module",
  "scripts": {
    "serve": "vite dev",
    "build": "yarn build:lib && yarn build:docs",
    "build:lib": "vite build",
    "build:docs": "vite build --config vite.docs.config.js",
    "test:unit": "vitest --run",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix lib",
    "deploy": "node deploy.mjs",
    "postpublish": "yarn deploy",
    "prepublishOnly": "yarn build:lib",
    "predeploy": "yarn build:docs",
    "test:unit:watch": "vitest"
  },
  "main": "./dist/murmur.umd.cjs",
  "module": "./dist/murmur.js",
  "files": [
    "lib/**/*",
    "dist/lib/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/murmur.js",
      "require": "./dist/murmur.umd.cjs"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "bootstrap": "^4.6.2",
    "clipboard": "^2.0.11",
    "d3": "7.6.1",
    "d3-geo-projection": "^4.0.0",
    "jsonp": "^0.2.1",
    "lodash": "4.17.21",
    "mutationobserver-shim": "^0.3.7",
    "querystring-es3": "^0.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "topojson": "^3.0.2",
    "vue-class-component": "^7.2.3",
    "vue-headroom": "^0.10.1",
    "vue-property-decorator": "^9.1.2",
    "vue-virtual-scroller": "^1.0.10"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@types/lodash": "^4.14.186",
    "@types/pym.js": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.2.2",
    "bootstrap-vue": "2.22.0",
    "chalk": "^4.1.2",
    "chroma-js": "^2.1.1",
    "eslint": "8",
    "eslint-plugin-vue": "8",
    "fast-glob": "^3.2.12",
    "front-matter": "^4.0.2",
    "get-sass-vars": "^3.4.0",
    "gh-pages": "^4.0.0",
    "highlight.js": "^11.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^21.1.0",
    "json-loader": "^0.5.7",
    "loader-utils": "^2.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-highlightjs": "^3.3.1",
    "ora": "^6.1.2",
    "sass": "^1.54.0",
    "sass-loader": "^10.1.0",
    "typescript": "~4.5.5",
    "url-join": "^4.0.1",
    "vite": "^4.0.4",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^0.28.3",
    "vue": "^2.7.14",
    "vue-cli-plugin-i18n": "^2.1.4",
    "vue-docgen-api": "^4.41.2",
    "vue-i18n": "^8.25.1",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "2.7.14",
    "vuex": "^3.6.2"
  },
  "peerDependencies": {
    "bootstrap-vue": "2.22.0",
    "vue": "^2.7.14",
    "vue-i18n": "^8.25.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2021": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "directories": {
    "lib": "lib",
    "doc": "docs"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "homepage": "https://icij.github.io/murmur/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/icij/murmur"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue",
    "./docs"
  ],
  "style": "dist/lib/murmur.css"
}
