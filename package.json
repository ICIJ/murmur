{
  "name": "icij-vue-collection",
  "version": "0.0.22",
  "description": "A collection of Vue component from ICIJ team",
  "author": "promera@icij.org",
  "main": "dist/lib/icij-vue-collection.umd.min.js",
  "files": [
    "src/**/*",
    "dist/lib/**/*"
  ],
  "private": false,
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "postpublish": "yarn deploy",
    "serve": "vue-cli-service serve",
    "build": "yarn build:docs && yarn build:lib",
    "build:docs": "vue-cli-service build --dest dist/docs",
    "build:lib": "vue-cli-service build --target lib --dest dist/lib 'src/main.js'",
    "deploy": "node deploy.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "es6-symbol": "^3.1.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "promise-polyfill": "^8.1.0",
    "vue": "^2.5.17",
    "vue-clipboard2": "^0.2.1",
    "vue-headroom": "^0.9.0",
    "vue-router": "^3.0.1",
    "vue-social-sharing": "^2.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.0",
    "ora": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "vue-slide-up-down": "^1.4.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
